#define _CRT_SECURE_NO_WARNINGS
#include <locale.h>
#include <stdio.h>
#include <math.h>
int main()
{
    setlocale(LC_ALL, "RUS");
    int g;   
    puts("Программа проверяет является ли введенный год високосным.");
    puts("Введите год\n");
    scanf_s("%d", &g);
    if (g % 4 != 0 || g % 100 != 0 && g % 400 == 0)
    {
        printf("Год не високосный\n");

    }
    else
    {
        printf("Год високосный\n");
    }

    
    int zn1;
    int zn2;
    int x;   
    puts(" программа для вычислений значения функции F(x).");
    puts("Введите число (x)\n");
    scanf("%d", &x);
    if (x <= 7)
    {
        zn1 = 3 * x - 9;
        printf("%d\n", zn1);
    }
    else
    {
        zn2 = (1 / (pow(x, 2) - 4));
        printf("%.3f\n", zn2);
    }


    int y;
    puts("Данная программа проверяет входит ли в него цифра 5.");
    puts("Введите трёхзначное число:\n");
    scanf("%d", &y);
    if (y < 1000)
    {
        if (y % 10 == 5 || y / 10 % 10 == 5 || y / 100 % 10 == 5)
        {
            puts("В данное число входит цифра 5\n");
        }
        else
        {
            puts("В данное число не входит цифра 5\n");
        }
    }
    else
    {
        printf("введите другое число\n");
        return(0);
    }
}
